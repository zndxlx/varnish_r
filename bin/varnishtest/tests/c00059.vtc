varnishtest "test ban obj.status"

server s1 {
	rxreq
	txresp -bodylen 5
	rxreq
	txresp -bodylen 3
} -start

varnish v1 -vcl+backend {} -start

client c1 {
	txreq
	rxresp
	expect resp.bodylen == 5
} -run

varnish v1 -cliok "ban obj.status == 201"
varnish v1 -cliok "ban obj.status == 200"
varnish v1 -cliok "ban.list"

client c1 {
	txreq
	rxresp
	expect resp.bodylen == 3
} -run
